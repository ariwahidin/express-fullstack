    <script>
        $(document).ready(function () {
            stopLoading();


            $('.menu-link').on('click', function () {
                const origin = window.location.origin;
                const path = $(this).data('url');
                const url = origin + path;
                const title = $(this).find('div[data-i18n]').text();
                console.log(url);


                if (path) {
                    startLoading();
                    $.ajax({
                        type: "GET",
                        url: url,
                        success: function (response) {
                            $('#titlePage').text(title);
                            stopLoading();
                            $(document).attr("title", title);
                            $('#app').html(response.content);
                            $('#userLoginName').text(response.user.fullname)
                            // Update URL dan simpan state di history
                            history.pushState({ content: response.content }, '', url);
                        }
                    })
                }
            })

            // Event listener untuk menangani tombol "Back"
            window.onpopstate = function (event) {
                if (event.state && event.state.content) {
                    $('#app').html(event.state.content);
                } else {
                    location.reload();
                }
            };
        });
    </script>
